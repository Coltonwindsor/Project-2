<div class="wrapper">
    <h1>All Characters</h1>

    <div class="nav">
        <a href="/character/new">
            <button>Create Character</button>
        </a>
        <a href="/game"><button>Games</button></a>
        <a href="/publisher"><button>Publishers</button></a>
    </div>
    <div class="list">

        {{#each allCharacters}}
        <a href="/character/{{_id}}">{{name}}
            <div>
                <img src="{{image}}" width="200px">
            </div>
        </a>
        {{/each}}

    </div>
</div>